OnAiTalk
{
	LastTalk = "\0\b[0]\s[0]"
	if WalletInstalled
	{
		if CHAIN.IDName == ""
		{
			if CriminalAction; LastTalk += CriminalTalk
			else; LastTalk += "\_q\![no-autopause]" + RandomTalk + "\_w[5000] "
		}
		else
		{
			LastTalk += ChainTalk
		}
	}
	else; LastTalk += "\_q\![no-autopause]" + PluginInstallMessage + "\_w[5000] "
	LastTalk
}

